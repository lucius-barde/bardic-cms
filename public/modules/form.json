{
	"name":	"Form",
	"scope": "pageElement",
	"icon": "fa fa-envelope",
	"baseFormat":{
		"order":{
			"type":"number",
			"name":"Order",
			"default":0,
			"min":0,
			"required":true
		},
		"mailto":{
			"type":"email",
			"name":"E-Mail",
			"placeholder":"contact@example.com"
			
		},
		"nonce":{
			"type":"number",
			"name":"Random number for more security",
			"min":0,
			"max":99
		}
	},

	"i18n":{
		"en":{
			"name": "Form",
			"namePlural": "Forms",
			"submit":"Envoyer"
		},
		"fr":{
			"name": "Formulaire",
			"namePlural": "Formulaires",
			"submit": "Envoyer"
		}
	}
}
